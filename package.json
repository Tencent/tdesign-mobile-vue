{
  "name": "tdesign-mobile-vue",
  "purename": "tdesign",
  "version": "0.4.0",
  "description": "tdesign-mobile-vue",
  "author": "TDesign Group",
  "homepage": "http://tdesign.tencent.com/vue-mobile/",
  "bugs": {
    "url": "/tdesign-mobile-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "/tdesign-mobile-vue.git"
  },
  "scripts": {
    "prepare": "husky install .husky",
    "dev": "cross-env NODE_ENV=development vite",
    "dev:debug": "cross-env NODE_ENV=development vite --debug",
    "prebuild": "rimraf dist/* lib/* es/* esm/* typings/*",
    "build": "cross-env NODE_ENV=production rollup -c scripts/rollup.config.js && npm run build:tsc",
    "build:tsc": "concurrently \"npm:build:tsc-*\"",
    "build:tsc-es": "tsc --emitDeclarationOnly -d --outDir es/",
    "build:tsc-esm": "tsc --emitDeclarationOnly -d --outDir esm/",
    "build:tsc-lib": " tsc --emitDeclarationOnly -d --outDir lib/",
    "site": "vite build",
    "site:preview": "vite build --mode preview",
    "test": "npm run test:unit",
    "test:update": "npm run test:unit-update && npm run test:node-update",
    "test:unit": "jest --config scripts/test/jest.unit.conf.js",
    "test:unit-update": "jest --config scripts/test/jest.unit.conf.js --updateSnapshot",
    "test:node": "jest --config scripts/test/jest.ssr.conf.js",
    "test:node-update": "jest --config scripts/test/jest.ssr.conf.js --updateSnapshot",
    "test:watch": "jest --config scripts/test/jest.unit.conf.js --watch",
    "test:coverage": "jest --config scripts/test/jest.unit.conf.js --coverage",
    "test:e2e": "cypress run --config-file scripts/test/cypress.json",
    "test:e2e-gui": "cypress open --config-file scripts/test/cypress.json",
    "lint": "eslint --ext .vue,.ts,.tsx ./ --max-warnings 0 --ignore-path .gitignore --ignore-path .eslintignore",
    "lint:fix": "npm run lint:fix-prettier && npm run lint:fix-eslint",
    "lint:fix-eslint": "eslint --ext .vue,.ts,.tsx ./ --max-warnings 0 --ignore-path .gitignore --ignore-path .eslintignore --fix",
    "lint:fix-prettier": "prettier ./ --ignore-path .gitignore --ignore-path .eslintignore --write './*.{vue,ts,tsx}' --ignore-unknown",
    "lint:vue": "vue-tsc --noEmit",
    "changelog": "publish-cli changelog",
    "robot": "publish-cli robot-msg",
    "commit": "git-cz"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "vue",
    "mobile",
    "tdesign"
  ],
  "title": "tdesign-mobile-vue",
  "main": "lib/index-lib.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "unpkg": "dist/tdesign.min.js",
  "jsdelivr": "dist/tdesign.min.js",
  "files": [
    "es",
    "esm",
    "lib",
    "dist",
    "typings",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "sideEffects": [
    "dist/*",
    "docs/**",
    "es/**/style/**",
    "esm/**/style/**"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.16.5",
    "@types/lodash": "^4.14.178",
    "lodash": "^4.17.21",
    "tdesign-icons-vue-next": "^0.0.4"
  },
  "peerDependencies": {
    "vue": "^3.2.4"
  },
  "devDependencies": {
    "@babel/core": ">=7.13.0 <8.0.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.16.5",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@popperjs/core": "^2.6.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-url": "^6.0.0",
    "@soerenmartius/vue3-clipboard": "^0.1.1",
    "@types/jest": "^27.0.3",
    "@types/lru-cache": "^5.1.0",
    "@types/markdown-it": "^12.0.1",
    "@types/markdown-it-anchor": "^7.0.0",
    "@types/markdown-it-container": "^2.0.3",
    "@types/prismjs": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.2.4",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/runtime-core": "^3.2.26",
    "@vue/test-utils": "^2.0.0-rc.17",
    "autoprefixer": "^10.2.4",
    "axios": "^0.21.0",
    "babel-jest": "^27.4.4",
    "commitizen": "^4.2.3",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "cypress": "^9.1.1",
    "cz-conventional-changelog": "3.3.0",
    "debug": "^4.3.1",
    "esbuild": "^0.14.3",
    "eslint": "^8.4.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "gray-matter": "^4.0.3",
    "husky": "^6.0.0",
    "jest": "^27.4.4",
    "jest-html-reporter": "^3.4.2",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^1.0.0",
    "less": "^4.1.2",
    "lint-staged": "^10.5.4",
    "lru-cache": "^6.0.0",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^6.0.1",
    "markdown-it-container": "^2.0.0",
    "markdown-it-toc-done-right": "^4.2.0",
    "mockdate": "^3.0.5",
    "postcss": "^8.2.1",
    "prettier": "^2.5.1",
    "prismjs": "^1.23.0",
    "qrcode.vue": "^3.2.2",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-multi-input": "1.1.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-static-import": "^0.1.1",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "rollup-pluginutils": "^2.8.2",
    "tdesign-icons-vue-next": "^0.0.4",
    "tdesign-publish-cli": "^0.0.9",
    "tdesign-site-components": "^0.5.35",
    "ts-jest": "^27.1.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2",
    "vite": "^2.7.2",
    "vite-plugin-tdoc": "^2.0.1",
    "vue": "^3.2.4",
    "vue-eslint-parser": "^8.0.1",
    "vue-router": "4.0.11",
    "vue-tsc": "^0.29.8",
    "vue3-jest": "^27.0.0-alpha.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
